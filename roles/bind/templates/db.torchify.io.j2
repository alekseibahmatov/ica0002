$TTL	604800
torchify.io.	IN	SOA	vm1.torchify.io. aleksei.torchify.io. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
torchify.io.	IN	NS	alekseibahmatov-1
{% for vm in groups['all'] %}
{{ vm }}	IN	A	{{ hostvars[vm]['ansible_default_ipv4']['address'] }}
{% endfor %}
{% for db in groups['db_servers'] %}
db	IN	A	{{ hostvars[db]['ansible_default_ipv4']['address'] }}
{% endfor %}
